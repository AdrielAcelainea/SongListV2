<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/img"
    android:backgroundTint="#3BE3E3E3"
    android:backgroundTintMode="src_over"
    tools:context=".SongDisplay">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Song List"
        android:textSize="45dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:gravity="center"
        android:fontFamily="@font/soulfunkdemo"/>

    <Button
        android:id="@+id/fivestarbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="@string/starfive"
        android:fontFamily="@font/punchliner"
        android:textColor="@color/black"
        android:textSize="17dp"/>

    <ListView
        android:id="@+id/displayList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="18dp"/>
</LinearLayout>